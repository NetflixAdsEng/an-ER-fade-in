{
  "name": "@netflixadseng/wc-netflix-cta",
  "version": "4.0.0",
  "description": "Netflix CTA Web Component",
  "scripts": {
    "postinstall": "cd src/ && npm install",
    "init": "node lib/api.js --cmd install",
    "updateDeps": "cd src/ && npm update",
    "build": "sh lib/confirm_env.sh 'node lib/api.js --cmd build'",
    "localhost": "sh lib/confirm_env.sh 'http-server ./ -p 8000'",
    "dist": "npm run updateDeps && sh lib/confirm_env.sh 'node lib/api.js --cmd dist'",
    "pub": "npm install && npm run gitClean && cd ./dist/npm && npm run pub",
    "syncVersion": "node lib/api.js --cmd sync && npm run track && npm run dist && npm run commitRelease",
    "track": "node lib/api.js --cmd track",
    "gitClean": "git add -A && git commit -m 'commits misc before publish' || true && git push origin master",
    "commitRelease": "git add . && VER=`git describe --abbrev=0` && git commit -m $VER"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NetflixAdsEng/wc-netflix-cta.git"
  },
  "main": "./src/main.js",
  "devDependencies": {
    "archiver": "^1.3.0",
    "babel-eslint": "^8.2.3",
    "debug": "git+https://github.com/gmcdev/debug.git",
