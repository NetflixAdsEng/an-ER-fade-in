    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "github-current-user": "^2.5.0",
    "handlebars": "^4.0.10",
    "http-server": "^0.11.1",
    "jszip": "^3.1.5",
    "minimist": "^1.2.0",
    "moment": "^2.22.1",
    "muffle": "^2.0.1",
    "ncp": "^2.0.0",
    "prettier": "^1.9.2",
    "prettier-eslint": "^8.7.1",
    "rimraf": "^2.6.1",
    "uglify-js": "^3.4.0",
    "uuid": "^3.0.1",
    "varname": "^2.0.3",
    "walk": "^2.3.9"
  },
